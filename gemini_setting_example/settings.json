{
  "mcpServers": {
    "poke": {
      "command": "uv",
      "args": [
        "run",
        "--with",
        "fastmcp",
        "fastmcp",
        "run",
        "Pokemon-MCP-Server/poke.py"
      ]
    },
    "opgg-mcp": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "supergateway",
        "--streamableHttp",
        "https://mcp-api.op.gg/mcp"
      ]
    },
    "anilist": {
      "command": "npx",
      "args": ["-y", "anilist-mcp"]
    }
  }
}